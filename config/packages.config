<?xml version="1.0" encoding="utf-8"?>
<!--
This is a Chocolatey packages.config file.
Save this file as 'packages.config' and run 'choco install packages.config' from the same directory.
This will install all the packages listed below.
-->
<packages>
  <!-- GNU Make: A tool which controls the generation of executables and other non-source files of a program. -->
  <package id="make" />

  <!-- Info-ZIP: A set of tools for handling ZIP archives. -->
  <package id="unzip" />

  <!-- fd: A simple, fast and user-friendly alternative to 'find'. -->
  <package id="fd" />

  <!-- ripgrep: A line-oriented search tool that recursively searches your current directory for a regex pattern. -->
  <package id="ripgrep" />

  <!-- MinGW-w64: A complete runtime environment for GCC to support binaries native to Windows 64-bit and 32-bit. -->
  <package id="mingw" />

  <!-- Sioyek: A PDF viewer designed for reading research papers and technical books. -->
  <package id="sioyek" />
</packages>
